<div class='container'>
  <div class='row'>
    <div class="col-sm-12 col-md-10 col-lg-8 px-5 px-sm-0 mx-auto">
      <h1>Room一覧</h1>
      <table class='table table-hover table-inverse'>
        <tbody>
          <% @entries.each do |e| %>
            <tr>
              <td>
                <%= image_tag e.user.get_profile_image, size:'50x50' %>
              </td>
              <td><%= e.user.name %>さんとのチャット</td>
              <td><%= link_to "Go chat", message_path(e.user) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>